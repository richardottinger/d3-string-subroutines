SUBROUTINE STRING.WRAP(SOMETHING,ANYTHING,WRAPPED)
   ANYTHING = ANYTHING[1,1]
   CALL STRING.FIRST(ANYTHING,1,FIRST.CHAR
   BEGIN CASE 
      CASE ANYTHING EQ '{'
         WRAPPED = ANYTHING : SOMETHING : '}'
      CASE ANYTHING EQ '['
         WRAPPED = ANYTHING : SOMETHING : ']'
      CASE ANYTHING EQ '<'
         WRAPPED = ANYTHING : SOMETHING : '>'
      CASE ANYTHING EQ '('
         WRAPPED = ANYTHING : SOMETHING : ')'
      CASE 1
         WRAPPED = ANYTHING : SOMETHING : ANYTHING
   END CASE
RETURN